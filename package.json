{
  "name": "track-viz",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite --host",
    "build": "tsc && vite build",
    "preview": "vite preview --host",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx}\"",
    "lint": "eslint \"src/**/*.{js,jsx,ts,tsx}\" --quiet",
    "bundleViz": "vite-bundle-visualizer --output stats.html",
    "jsonSchema": "typescript-json-schema --ignoreErrors \"./src/**/*.ts\" ExportData -o \"./public/trackVizFileSchema.json\""
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.9.2",
    "@tinymce/tinymce-react": "^4.3.0",
    "classnames": "^2.2.6",
    "focus-visible": "^5.1.0",
    "gsap": "^3.11.4",
    "joypad.js": "^2.3.2",
    "lodash": "^4.17.15",
    "react": "^18.2.0",
    "react-accessible-accordion": "^5.0.0",
    "react-color": "^2.19.3",
    "react-dom": "^18.2.0",
    "react-icons": "^3.10.0",
    "react-redux": "^7.2.0",
    "sanitize-filename": "^1.6.3",
    "textures": "^1.2.3",
    "three": "^0.147.0",
    "webxr-polyfill": "^2.0.3"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.191",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.10",
    "@vitejs/plugin-react": "^3.0.1",
    "eslint": "^8.31.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-import": "^2.27.4",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.32.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "prettier": "^2.8.2",
    "sass": "^1.58.0",
    "typescript": "^4.9.4",
    "typescript-json-schema": "^0.55.0",
    "typescript-plugin-css-modules": "^4.1.1",
    "vite": "^4.0.4",
    "vite-bundle-visualizer": "^0.4.2",
    "vite-plugin-mkcert": "^1.11.0",
    "vite-plugin-svgr": "^2.4.0"
  },
  "license": "UNLICENSED"
}
